@model TejInfraFollowUp.Models.Lead

@{
    ViewBag.Title = "EmployeeDashBoard";
    Layout = "~/Views/Shared/EmployeeLayout.cshtml";
}
<div class="page-wrapper">

    <div class="page-breadcrumb">
        <div class="row">
            <div class="col-5 align-self-center">
                <h4 class="page-title"></h4>
                <div class="d-flex align-items-center">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="#">DashBoard</a></li>
                            <li class="breadcrumb-item active" aria-current="page">DashBoard </li>
                        </ol>
                    </nav>
                </div>
            </div>

        </div>
    </div>
    <div class="auth-wrapper d-flex no-block justify-content-center align-items-center" style="background: url(../../assets/images/big/auth-bg.jpg) no-repeat center center;min-height: 55vh;display: table-cell;">
        <div class="auth-box" style="max-width:1286px;margin:0%">
            <div id="loginform">
                <div class="logo">

                    <h5 class="font-medium m-b-20">Today's FollowUp</h5>
                </div>
            </div>
            <div class="row">
                <div class="col-12">
                    <div class="table-responsive">

                        <table id="file_export" class="table table-striped table-bordered display">
                            <thead>
                                <tr>

                                    <th>Contact Person</th>
                                    <th>Contact No</th>
                                    <th>Interaction Date</th>
                                    <th>FollowUp Date</th>
                                    <th>Expected Product</th>
                                    <th>Data Source</th>
                                    <th>Executive Name</th>
                                    <th>Mode Of Interaction</th>

                                    <th>Description </th>

                                </tr>
                            </thead>
                            <tbody>
                                @if (Model != null)
                                {
                                    if (Model.lstLead != null)
                                    {
                                        foreach (var item in Model.lstLead)
                                        {
                                            <tr>
                                                <td>@item.Fk_ProcpectId</td>
                                                <td>@item.ContactNo</td>
                                                <td>@item.FirstInstructionDate</td>
                                                <td>@item.FollowupDate</td>
                                                <td>@item.Fk_ExpectedProductCategoryId</td>
                                                <td>@item.Fk_SourceId</td>
                                                <td>@item.Fk_ExecutiveId</td>
                                                <td>@item.Fk_ModeInterActionId</td>

                                                <td>@item.Description</td>
                                            </tr>
                                        }
                                    }
                                }

                            </tbody>
                        </table>
                    </div>
                </div>


            </div>
        </div>
    </div>

    <div class="auth-wrapper d-flex no-block justify-content-center align-items-center" style="background: url(../../assets/images/big/auth-bg.jpg) no-repeat center center;min-height: 55vh;display: table-cell;">
