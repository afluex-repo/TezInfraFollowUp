@model TejInfraFollowUp.Models.DWR

@{
    ViewBag.Title = "ListDWR";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="page-wrapper">

    <div class="page-breadcrumb">
        <div class="row">
            <div class="col-5 align-self-center">
                <h4 class="page-title"></h4>
                <div class="d-flex align-items-center">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="#">DWR</a></li>
                            <li class="breadcrumb-item active" aria-current="page">Lead List </li>
                        </ol>
                    </nav>
                </div>
            </div>

        </div>
    </div>
    @using (Html.BeginForm("ListDWR", "DWR", FormMethod.Post, new { @id = "signupForm", enctype = "multipart/form-data" }))
    {
        <div class="alert alert-success" style="display:@ViewBag.saverrormsg">
            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">×</span> </button>
            <h5 class="text-success"><i class="fa fa-check-circle"></i>@TempData["Error"]</h5>

        </div>
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                           @*  <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group row">
                                            <label class="control-label text-right col-md-3">User Type Name</label>
                                            <div class="col-md-9">
                                                @Html.TextBoxFor(m => m.Fk_UserTypeId, new { @class = "form-control form-control-lg", @Placeholder = "Customer Name" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group row">
                                            <label class="control-label text-right col-md-3">Employee Name </label>
                                            <div class="col-md-9">
                                                @Html.TextBoxFor(m => m.Name, new { @class = "form-control form-control-lg", @Placeholder = "Customer Code" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group row">
                                            <label class="control-label text-right col-md-3">Contact Number </label>
                                            <div class="col-md-9">
                                                @Html.TextBoxFor(m => m.ContactNo, new { @class = "form-control form-control-lg", @Placeholder = "Customer Code" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group row">

                                            <div class="col-md-9">
                                                <input type="submit" value="Get Details" class="btn btn-primary" name="GetDetails" />

                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->

                                    <!--/span-->
                                </div>*@
                        <div class="table-responsive">
                            <table id="file_export" class="table table-striped table-bordered display">
                                <thead>
                                    <tr>
                                         <th>Action</th>
                                        <th>Prospect Name</th>
                                        <th>Lead Description</th>
                                         <th>Next FollowUp Date</th>
                                         <th>Remark</th>
                                         <th>Business Chance</th>
                                         <th>Mode Of Interaction</th>
                                         <th>Executive Name</th>
                                         <th>Meeting Date And Time </th>
                                        
                                       
                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model != null)
                                    {
                                        if (Model.lstDWR != null)
                                        {
                                            foreach (var item in Model.lstDWR)
                                            {
                                        <tr>  @Html.HiddenFor(m => m.Pk_DwrId)
                                             <td>
                                                <a href="/DWR/DeleteDWR?Pk_LeadeId=@item.Pk_DwrId" class="label label-primary"  onclick="return confirm('Are you want to delete?;')">Delete</a>
                                                &nbsp;
                                         @*   <a href="/DWR/ListDWR?Pk_LeadeId=@item.Pk_DwrId" class="label label-primary"  onclick>Add Comment</a>
                                                &nbsp;*@
                                            </td>
                                          
                                            <td>@item.Fk_ProcpectId</td>
                                            <td>@item.Fk_ExpectedProductCategoryId</td>
                                            <td>@item.NextFollowupDate</td>
                                            <td>@item.Reemark</td>
                                            <td>@item.Fk_BusinessChanceId</td>
                                            <td>@item.Fk_ModeInterActionId</td>
                                            <td>@item.Fk_ExecutiveId</td>
                                            <td>@item.MeetingDate,@item.MeetingTime</td>
                                           
                                        
                                          
                                        </tr>
                                            }
                                        }
                                    }

                                </tbody>                        
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }



